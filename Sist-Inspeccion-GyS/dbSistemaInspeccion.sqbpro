	CONSTRAINT FKLUCES_ENTRADA FOREIGN KEY (idluces) REFERENCES Luces (idluces),
	CONSTRAINT FKPERIFERICOS_ENTRADA FOREIGN KEY (idperifericos) REFERENCES Perifericos (idperifericos),
	CONSTRAINT FKLLANTAS_ENTRADA FOREIGN KEY (idllantas) REFERENCES Llantas (idllantas),
	CONSTRAINT FKCOMPARTIMIENTOS_ENTRADA FOREIGN KEY (idcompartimientosocultos)	REFERENCES Compartimientosocultos (idcompartimientosocultos)	
)

