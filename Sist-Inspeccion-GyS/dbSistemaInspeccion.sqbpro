	&quot;idllantas&quot; INTEGER NOT NULL, 
	&quot;idcompartimientosocultos&quot; INTEGER NOT NULL,
	
	CONSTRAINT PKINSPECCIONENTRADA PRIMARY KEY (&quot;folio&quot;), 
