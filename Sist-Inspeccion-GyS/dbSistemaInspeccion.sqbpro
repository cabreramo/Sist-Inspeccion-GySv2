	&quot;unidadmedida&quot;	TEXT NOT NULL,
	&quot;temperaturaprogramada&quot;	NUMERIC NOT NULL,
	&quot;temperaturareal&quot;	NUMERIC NOT NULL,
	&quot;idimagenes&quot; INTEGER NOT NULL,
	&quot;idluces&quot; INTEGER NOT NULL, 
